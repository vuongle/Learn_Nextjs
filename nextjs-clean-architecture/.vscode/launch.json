{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: debug server",
      "type": "pwa-node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/next/dist/bin/next",
      "args": ["dev"],
      "runtimeExecutable": "node",
      "runtimeArgs": ["--inspect"],
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "trace": true
    }
  ]
}
